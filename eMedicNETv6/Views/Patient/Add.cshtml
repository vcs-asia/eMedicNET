@model eMedicEntityModel.Models.v1.PatientNextKinGuarantor
@{
    ViewData["Title"] = "Patient";
}
@section css
{
    <link href="~/assets/css/form.min.css" rel="stylesheet">
}
@section js
{
    <script src="~/assets/js/bundles/ckeditor/ckeditor.js"></script>
    <script src="~/assets/js/form.min.js"></script>
    <!-- Custom Js -->
    <script src="~/assets/js/bundles/jquery-steps/jquery.steps.min.js"></script>
    <script src="~/assets/js/admin.js"></script>
    <script src="~/assets/js/pages/forms/form-wizard.js"></script>
    <script src="~/assets/js/pages/forms/advanced-form-elements.js"></script>
    <script src="~/assets/js/pages/forms/editors.js"></script>
}
<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <ul class="breadcrumb breadcrumb-style">
                        <li>
                            <h4 class="page-title">@ViewBag.Title</h4>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- Horizontal Layout -->
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <h2>
                            <strong>Add</strong>
                        </h2>
                    </div>
                    <div class="body">
                        <form id="wizard_with_validation" asp-action="Add" enctype="multipart/form-data" method="post">
                            <h3>Primary Information</h3>
                            <fieldset>
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="row clearfix">
                                    <div class="col-md-9">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnPname" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnPname"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnIcpno" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnIcpno"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <select asp-for="Patient.PrnGendr" asp-items="ViewBag.PrnGendr" class="form-control"></select>
                                                <label class="form-label" asp-for="Patient.PrnGendr"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input type="date" asp-for="Patient.PrnDtdob" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnDtdob"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <select asp-for="Patient.PrnMrgst" asp-items="ViewBag.PrnMrgst" class="form-control"></select>
                                                <label class="form-label" asp-for="Patient.PrnMrgst"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input type="date" asp-for="Patient.PrnRegdt" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnRegdt"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnRegno" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnRegno"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnFoldr" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnFoldr"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <select asp-for="Patient.PrnNtion" asp-items="ViewBag.PrnNtion" class="form-control"></select>
                                                <label class="form-label" asp-for="Patient.PrnNtion"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-6">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnTelhp" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnTelhp"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnEmail" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnEmail"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-6">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnRefby" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnRefby"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnRefcl" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnRefcl"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-12">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnAddr1" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnAddr1"></label>
                                            </div>
                                            <div class="help-info">House No. /Building Name/ Jalan</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-6">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnAddr2" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnAddr2"></label>
                                            </div>
                                            <div class="help-info">Postal Code, City</div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="Patient.PrnAddr3" class="form-control">
                                                <label class="form-label" asp-for="Patient.PrnAddr3"></label>
                                            </div>
                                            <div class="help-info">State</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-12">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <select asp-for="Patient.PrnPanel" asp-items="ViewBag.PrnPanel" class="form-control"></select>
                                                <label class="form-label" asp-for="Patient.PrnPanel"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-12">
                                        <label class="form-label" asp-for="Patient.PrnTrpln"></label>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-12">
                                        <textarea id="ckeditor" asp-for="Patient.PrnTrpln"></textarea>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-lg-2 col-md-2 col-sm-4 col-xs-5">
                                    </div>
                                    <div class="col-lg-10 col-md-10 col-sm-8 col-xs-7">
                                        <input type="hidden" asp-for="Patient.PrnAutid" value="0" />
                                        <input type="hidden" asp-for="Patient.PrnUsrid" value="@eMedicEntityModel.Models.v1.SessionData.Username" />
                                        <input type="hidden" asp-for="Patient.PrnCdate" value="@DateTime.Now" />
                                        <button type="submit" class="btn btn-primary m-t-15 waves-effect">SAVE</button>
                                    </div>
                                </div>
                            </fieldset>
                            <h3>Next of Kin</h3>
                            <fieldset>
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="row clearfix">
                                    <div class="col-md-9">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="PatientNextKin.PknSname" class="form-control">
                                                <label class="form-label" asp-for="PatientNextKin.PknSname"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="PatientNextKin.PknSicno" class="form-control">
                                                <label class="form-label" asp-for="PatientNextKin.PknSicno"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input asp-for="PatientNextKin.PknOccup" class="form-control" />
                                                <label class="form-label" asp-for="PatientNextKin.PknOccup"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <textarea type="date" asp-for="PatientNextKin.PknAddrs" rows="4" class="form-control"></textarea>
                                                <label class="form-label" asp-for="PatientNextKin.PknAddrs"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <select asp-for="PatientNextKin.PknReltn" asp-items="ViewBag.PrnReltn" class="form-control"></select>
                                                <label class="form-label" asp-for="PatientNextKin.PknReltn"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group form-float">
                                            <div class="form-line">
                                                <input type="date" asp-for="PatientNextKin.PknTelno" class="form-control">
                                                <label class="form-label" asp-for="PatientNextKin.PknTelno"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-lg-2 col-md-2 col-sm-4 col-xs-5">
                                    </div>
                                    <div class="col-lg-10 col-md-10 col-sm-8 col-xs-7">
                                        <input type="hidden" asp-for="PatientNextKin.PknPatid" value="0" />
                                        <input type="hidden" asp-for="PatientNextKin.PknUsrid" value="@eMedicEntityModel.Models.v1.SessionData.Username" />
                                        <input type="hidden" asp-for="PatientNextKin.PknCdate" value="@DateTime.Now" />
                                        <button type="submit" class="btn btn-primary m-t-15 waves-effect">SAVE</button>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- #END# Horizontal Layout -->
    </div>
</section>